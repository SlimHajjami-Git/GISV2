# Production Environment Variables
# Copy this file to .env.prod and fill in your secure values

# PostgreSQL
POSTGRES_PASSWORD=ChangeThisToASecurePassword123!

# RabbitMQ
RABBITMQ_USER=gisadmin
RABBITMQ_PASSWORD=ChangeThisToASecurePassword456!

# JWT Secret (minimum 32 characters)
JWT_SECRET_KEY=YourSuperSecretKeyForProductionAtLeast32CharactersLong!

# Optional: Domain name for SSL
DOMAIN=yourdomain.com
